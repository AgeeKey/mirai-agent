# Environment variables for GitHub Secrets setup
# Copy this file to secrets.env and fill in your actual values
# Then run: source secrets.env && ./scripts/setup-github-secrets.sh

# GitHub / GHCR
export GH_TOKEN="your_github_token_here"
export GHCR_USERNAME="AgeeKey"
export GHCR_TOKEN="your_ghcr_token_here"

# SSH / Server
export SSH_HOST="your_server_ip_here"
export SSH_USER="root"
export SSH_KEY="-----BEGIN OPENSSH PRIVATE KEY-----
your_ssh_private_key_here
-----END OPENSSH PRIVATE KEY-----"

# Integration APIs
export OPENAI_API_KEY="your_openai_api_key_here"
export TELEGRAM_BOT_TOKEN="your_telegram_bot_token_here"
export TELEGRAM_CHAT_ID_ADMIN="your_telegram_chat_id_here"

# Trading APIs
export BINANCE_API_KEY="your_binance_api_key_here"
export BINANCE_API_SECRET="your_binance_secret_here"

# Domains
export DOMAIN_PANEL="aimirai.online"
export DOMAIN_STUDIO="aimirai.info"
export ENVIRONMENT="production"

# Security
export JWT_SECRET="your_jwt_secret_here"
export WEB_USER="admin"
export WEB_PASS="your_web_password_here"
export CODEX_TOKEN="your_codex_token_here"